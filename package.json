{
  "name": "proper-lockfile",
  "version": "3.0.2",
  "description": "A inter-process and inter-machine lockfile utility that works on a local or network file system",
  "keywords": [
    "lock",
    "locking",
    "file",
    "lockfile",
    "fs",
    "cross-process"
  ],
  "author": "Andr√© Cruz <andre@moxy.studio>",
  "homepage": "https://github.com/moxystudio/node-proper-lockfile",
  "repository": {
    "type": "git",
    "url": "git@github.com:moxystudio/node-proper-lockfile.git"
  },
  "license": "MIT",
  "main": "index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "jest --env node --coverage --runInBand",
    "prerelease": "npm t && npm run lint",
    "release": "standard-version",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "standard-version": {
    "scripts": {
      "posttag": "git push --follow-tags origin master && npm publish"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "graceful-fs": "^4.1.11",
    "retry": "^0.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@segment/clear-timeouts": "^2.0.0",
    "delay": "^3.0.0",
    "eslint": "^5.3.0",
    "eslint-config-moxy": "^6.0.1",
    "execa": "^0.11.0",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "lint-staged": "^7.0.0",
    "mkdirp": "^0.5.1",
    "p-defer": "^1.0.0",
    "rimraf": "^2.6.2",
    "stable": "^0.1.8",
    "standard-version": "^4.4.0"
  }
}
